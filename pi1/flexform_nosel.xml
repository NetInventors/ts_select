<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:ts_select/pi1/locallang.xml:ff.title.general</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <selObj>
            <TCEforms>
              <label>LLL:EXT:ts_select/pi1/locallang.xml:ff.display</label>
              <config>
			    <type>select</type>
				<itemsProcFunc>tx_tsselect_flexFormFields->addFields</itemsProcFunc>
			  </config>
            </TCEforms>
          </selObj>   
        </el>
      </ROOT>
    </sDEF>
    <sEXTRA>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:ts_select/pi1/locallang.xml:ff.title.extra</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
	  <text>
	    <TCEforms>
	      <label>LLL:EXT:ts_select/pi1/locallang.xml:ff.extra.textTitle</label>
	      <config>
	        <type>text</type>
	      </config>
	    </TCEforms>
	  </text>
	  <object>
	    <TCEforms>
	      <label>LLL:EXT:ts_select/pi1/locallang.xml:ff.extra.objTitle</label>
	      <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>*</allowed>
                <size>1</size>
                <maxitems>1</maxitems>
                <minitems>0</minitems>
                <show_thumbs>0</show_thumbs>
	      </config>
	    </TCEforms>
	  </object>
	  <file>
	    <TCEforms>
	      <label>LLL:EXT:ts_select/pi1/locallang.xml:ff.extra.fileTitle</label>^M
	      <config>
	        <type>group</type>
	        <internal_type>file</internal_type>
	        <allowed>*</allowed>
	        <max_size>10000</max_size>
	        <uploadfolder>uploads/tx_tsselect/</uploadfolder>
	        <size>1</size>
	        <maxitems>1</maxitems>
	        <minitems>0</minitems>
	        <show_thumbs>1</show_thumbs>
	      </config>^M
	    </TCEforms>
	  </file>
        </el>
      </ROOT>
    </sEXTRA>
  </sheets>
</T3DataStructure>